        "width": "@L",
        "height": "@W",
        "depth": "@H"
      }
    },
    {
      "id": "shell",
      "type": "Features::Shell",
      "position": { "x": 300, "y": 100 },
      "inputs": {
        "shape": { "nodeId": "base", "socketId": "shape" }
      },
      "params": {
        "thickness": "@wall",
        "inside": true
      }
    },
    {
