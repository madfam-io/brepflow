      "type": "Transform::ArrayLinear",
      "position": { "x": 300, "y": 250 },
      "inputs": {
        "shape": { "nodeId": "boss1", "socketId": "shape" }
      },
      "params": {
        "count": 4,
        "spacing": { "x": 30, "y": 0, "z": 0 }
      }
    },
    {
      "id": "union",
      "type": "Boolean::Union",
      "position": { "x": 500, "y": 175 },
      "inputs": {
        "shapes": [
          { "nodeId": "shell", "socketId": "shape" },
          { "nodeId": "boss_pattern", "socketId": "shapes" }
        ]
      },
      "params": {
        "simplify": true
      }
    },
    {
      "id": "fillet",
      "type": "Features::Fillet",
      "position": { "x": 700, "y": 175 },
      "inputs": {
        "shape": { "nodeId": "union", "socketId": "shape" }
      },
      "params": {
        "radius": 2,
        "selectAll": false
      }
    }
