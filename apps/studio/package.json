    "@brepflow/nodes-core": "workspace:^",
    "@brepflow/types": "workspace:*",
    "@brepflow/viewport": "workspace:*",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "clsx": "^2.1.1",
    "comlink": "^4.4.1",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.544.0",
    "react": "^18.2.0",
    "react-confetti": "^6.4.0",
    "react-dom": "^18.2.0",
