{
  "buildCommand": "pnpm --filter @brepflow/studio^... run build && pnpm --filter @brepflow/studio run build",
  "installCommand": "pnpm install",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/studio"
    }
  ],
  "functions": {
    "api/studio.js": {
      "maxDuration": 30
    }
  }
}