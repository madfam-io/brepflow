{
  "root": "apps/studio",
  "buildCommand": "cd ../.. && pnpm --filter @brepflow/studio... run build",
  "installCommand": "cd ../.. && pnpm install",
  "outputDirectory": "dist",
  "framework": null,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cross-Origin-Opener-Policy",
          "value": "same-origin"
        },
        {
          "key": "Cross-Origin-Embedder-Policy",
          "value": "require-corp"
        }
      ]
    }
  ]
}